{
    "endpoint": "https://search-logon-order-search-qyhfl5jwmhiaatf23g3wafmjia.eu-west-1.es.amazonaws.com/orders",
    "title": "Orders Search Engine",
    "searchbox": {
        "queryFields": ["ORDER_NUMBER","ORDER_NUMBER_FROM_SEQ_USAGE", "LDS_DELIVERY_NOTE_NO"],
        "placeholder": "Logon Order, Sequential or LDS Number"
    },
    "filters": {
        "cityCheckbox": {
            "fields": "LNF_SITE_CITY.keyword",
            "title": "Top Cities",
            "id": "cities"
        },
        "cityList": {
            "fields": ["LNF_SITE_CITY.keyword", "SHIPPINGPOINT_ID.keyword"],
            "title": "Cities",
            "id": "cities"
        },
        "searchboxCity": {
            "title": "More cities",
            "placeholder": "Search city",
            "fields": "LNF_SITE_CITY",
            "id": "more_cities"
        },
        "searchboxPerson": {
            "title": "Contact Signature",
            "placeholder": "Contact person",
            "fields": "CONTACT_PERSON_SIGNATURE_TXT",
            "id": "person"
        },
        "dates": {
            "fields": "ACTUAL_DELIVERY_DAT",
            "title": "Delivery Dates:",
            "id": "event_date_filter"
        },
        "deliveryFrom": {
            "fields": "DELIVERY_FROM_DAT",
            "title": "Delivery From Dates:",
            "id": "event_date_filter"
        },
        "deliveryTo": {
            "fields": "DELIVERY_TO_DAT",
            "title": "Delivery To Dates:",
            "id": "event_date_filter"
        },
        "searchboxTransporter": {
            "title": "Transporter",
            "placeholder": "Search Commercial or Executing Carrier",
            "fields": ["COMM_CARRIER_ID", "EXEC_CARRIER_ID"],
            "id": "transporter"
        },
        "searchboxBusiness": {
            "title": "Business Partners",
            "placeholder": "Search Destination, Customer, Address or Payer",
            "fields": ["SHIPTO_SAP_BP_ID", "SOLDTO_SAP_BP_ID", "BILLTO_SAP_BP_ID", "PAYER_SAP_BP_ID"],
            "id": "business_partners"
        },
        "searchboxCreatedBy": {
            "title": "Created By",
            "placeholder": "Search person",
            "fields": "CTL_CRE_UID",
            "id": "crated_by"
        },
        "onHoldCheckbox": {
            "fields": "ON_HOLD_ORDER_AND_LOCKED_FLAG",
            "title": "On Hold",
            "id": "onHold"
        }
    },
    "sortingSelector": {
        "options": [
            {
                "label": "Latest Delivery Date",
                "field": "ACTUAL_DELIVERY_DAT",
                "order": "desc"
            },
            {
                "label": "Earliest Delivery Date",
                "field": "ACTUAL_DELIVERY_DAT",
                "order": "asc"
            }
        ]
    },
    "samples": {
        "grid": {
            "fields": ["ACTUAL_DELIVERY_DAT", "ORDER_ID", "LNF_SITE_CITY", "SHIPPINGPOINT_ID", "CONTACT_PERSON_SIGNATURE_TXT", "COMMERCIAL_DISTANCE_M", "DELIVERY_TYPE_CD", "TRANSPORT_DISTANCE_M", "TRANSPORT_DURATION_MIN"],
            "key": "grid",
            "title": "Grid"
        },
        "list": {
            "fields": ["ORDER_ID", "ACTUAL_DELIVERY_DAT", "SHIPPINGPOINT_ID", "DELIVERY_TYPE_CD", "LNF_SITE_CITY", "CONTACT_PERSON_SIGNATURE_TXT", "COMMERCIAL_DISTANCE_M", "TRANSPORT_DISTANCE_M", "TRANSPORT_DURATION_MIN"],
            "key": "list",
            "title": "List"
        },
        "table": {
            "fields": ["ORDER_ID", "ACTUAL_DELIVERY_DAT", "SHIPPINGPOINT_ID", "DELIVERY_TYPE_CD", "LNF_SITE_CITY", "CONTACT_PERSON_SIGNATURE_TXT", "COMMERCIAL_DISTANCE_M", "TRANSPORT_DISTANCE_M", "TRANSPORT_DURATION_MIN"],
            "key": "table",
            "title": "Table"
        },
        "noResults": {
            "NoHits.NoResultsFound":"No results were found for {query}"
        }
    },
    "dateFilter": {
        "startDatePlaceholder": "Select start date",
        "endDatePlaceholder": "Select end date"
    }
}
